						 	<h:panelGroup>
								<h:outputText value="Empresa: " styleClass="bold"/>
							</h:panelGroup>
					        <h:outputText value="#{chargeMassiveTagsBean.companyTagName}" style="width: 200px; display: inline-block;"/>
