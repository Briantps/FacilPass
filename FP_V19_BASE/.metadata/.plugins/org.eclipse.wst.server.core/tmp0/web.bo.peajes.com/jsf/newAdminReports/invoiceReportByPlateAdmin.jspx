<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:a4j="http://richfaces.org/a4j">
<head>
	<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8" />
	<link rel="stylesheet" href="/web.bo.peajes.com/css/blueprint/screen.css" type="text/css" media="screen, projection" />
	<link rel="stylesheet" href="/web.bo.peajes.com/css/blueprint/print.css" type="text/css" media="print" />
	<link rel="stylesheet" href="/web.bo.peajes.com/css/styles.css" type="text/css" media="screen, projection" />
	<script language="javascript" type="text/javascript" src="/web.bo.peajes.com/js/validation.js"></script>
	<title>Recibo Consolidado de Peajes</title>
</head>
<body>
<div class="container">
<f:view>
<f:subview id="header"><ui:include src="../content/header.jsp"/></f:subview>
<div class="span-24">
	<f:subview id="headerMenu"><ui:include src="../menu/menu.jsp"/></f:subview>
</div>
<div class="span-5">
	<ui:include src="/jsf/menu/sideMenu.jsp"/>
</div>
<div class="bkContents span-18 prepend-1">
    <f:loadBundle basename="staticText.messages" var="msg" />
	<rich:spacer height="5"/>	

    <h:form>
      <rich:panel>
        <f:facet name="header">
          <h:outputText value="Recibo Consolidado de Peajes"/>
        </f:facet>

        <h:panelGrid columns="2">
        <h:panelGroup>
          <rich:spacer width="10"/>
		  <h:outputText value="Tipo Doc: " styleClass="bold"/>
          <h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
		</h:panelGroup>

		<h:panelGroup>
          <rich:spacer width="10"/>
		  <h:selectOneMenu id="tyd" value="#{invoiceReportByPlateBeanAdmin.codeType}" styleClass="normal">
		    <f:selectItems value="#{invoiceReportByPlateBeanAdmin.codeTypesList}"/>
			  <a4j:support event="onchange" action="#{invoiceReportByPlateBeanAdmin.changeTypeDoc}" reRender="account,account2,codc,panelData"/>
			</h:selectOneMenu>
		</h:panelGroup>

		<h:panelGroup>
          <rich:spacer width="10"/>
          <rich:spacer height="25"/>
		  <h:outputText value="#{msg.NumDoc}: " styleClass="bold"/>
		  <h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
		</h:panelGroup>

		<h:panelGroup>
          <rich:spacer width="10"/>
		  <h:inputText id="codc" value="#{invoiceReportByPlateBeanAdmin.codeClient}" styleClass="normal"
						onkeypress="return numOnly(event)" maxlength="15" >
                    <a4j:support event="onkeyup" action="#{invoiceReportByPlateBeanAdmin.ocult1}" reRender="account,account2,panelData"/>
                    <a4j:support event="onmouseup" action="#{invoiceReportByPlateBeanAdmin.ocult1}" reRender="account,account2,panelData"/>
					<f:validator validatorId="blank"/>
		  </h:inputText>
          <rich:spacer width="5"/>
          <a4j:commandButton value="Buscar Cuentas" action="#{invoiceReportByPlateBeanAdmin.search}"
						reRender="outp1,outp2,ra,ra1,outp3,ra2,account,panel,panelData">
		</a4j:commandButton>
		</h:panelGroup> 

          <h:panelGroup>
          <a4j:outputPanel id="outp1" style="height:20px;">
          <rich:panel id="ra" rendered="#{invoiceReportByPlateBeanAdmin.showAccounts1 || invoiceReportByPlateBeanAdmin.showAccounts2}" style="border:0;height:20px;">
		   <h:outputText value="Cuenta FacilPass:" styleClass="bold"/>
		   <h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
          </rich:panel>
          </a4j:outputPanel>
		 </h:panelGroup>

         <h:panelGroup>
         <a4j:outputPanel id="outp2" style="height:30px;">
         <rich:panel id="ra1" rendered="#{invoiceReportByPlateBeanAdmin.showAccounts1}" style="border:0;height:20px;">
           <h:selectOneMenu id="account" value="#{invoiceReportByPlateBeanAdmin.accountId}">
              <f:selectItems value="#{invoiceReportByPlateBeanAdmin.accountList}"></f:selectItems>
                <a4j:support event="onchange" action="#{invoiceReportByPlateBeanAdmin.ocult}" reRender="panelData,rich1"/>
 		   </h:selectOneMenu>
         </rich:panel>
         </a4j:outputPanel>

         <a4j:outputPanel id="outp3" style="height:30px;">
         <rich:panel id="ra2" rendered="#{invoiceReportByPlateBeanAdmin.showAccounts2}" style="border:0;height:30px;">
           <h:inputText id="account2" value="#{invoiceReportByPlateBeanAdmin.accountId}" style="font-weight:bold; color:black;" disabled="true"/>
         </rich:panel>
         </a4j:outputPanel>
         </h:panelGroup>
    
         <h:panelGroup>
           <rich:spacer width="10"/>
           <rich:spacer height="35"/>
		   <h:outputText value="Mes:" styleClass="bold"/>
		   <h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
		 </h:panelGroup>

          <h:panelGroup> 
           <rich:spacer width="10"/>
           <h:selectOneMenu  value="#{invoiceReportByPlateBeanAdmin.month}">
              <f:selectItems value="#{invoiceReportByPlateBeanAdmin.monthList}"></f:selectItems>
                <a4j:support event="onchange" action="#{invoiceReportByPlateBeanAdmin.ocult}" reRender="panelData,rich1"/>
 		   </h:selectOneMenu>
          </h:panelGroup>

         <h:panelGroup>
           <rich:spacer width="10"/>
		   <h:outputText value="Año:" styleClass="bold"/>
		   <h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
		 </h:panelGroup>

         <h:panelGroup> 
           <rich:spacer width="10"/>
           <h:selectOneMenu  value="#{invoiceReportByPlateBeanAdmin.year}">
              <f:selectItems value="#{invoiceReportByPlateBeanAdmin.yearList}"></f:selectItems>
                <a4j:support event="onchange" action="#{invoiceReportByPlateBeanAdmin.ocult}" reRender="panelData,rich1"/>
 		   </h:selectOneMenu>
         </h:panelGroup>

         <h:panelGroup>
           
           <h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
		   <h:outputLabel value="#{msg.MandFields}" styleClass="loginInput"/>
		 </h:panelGroup>

         <h:panelGroup> 
         <rich:spacer width="10"/>
         <h:commandButton value="Buscar" action="#{invoiceReportByPlateBeanAdmin.validate}">
           <a4j:support event="oncomplete" reRender="panelData"/>
         </h:commandButton>
         </h:panelGroup>

        </h:panelGrid>

        <rich:spacer height="5"/>
        <a4j:outputPanel id="panelData">
				<rich:panel id="rich1" rendered="#{invoiceReportByPlateBeanAdmin.showData}">
				<rich:spacer width="5"/>
					<h:panelGrid style="width: 172px; margin: auto; padding:10px;" columns="2">
						<h:commandLink id="clinkpdf" target="_blank" value="" 
							action="#{invoiceReportByPlateBeanAdmin.printPdf}">
							<img title="Imprimir" src="/web.bo.peajes.com/img/generar_btn.gif" alt="Imprimir"
							style="cursor: pointer; border: 0px solid blue;" />
                            <f:setPropertyActionListener target="#{invoiceReportByPlateBeanAdmin.exportOption}" value="PDF"/>
		           		</h:commandLink>
                        <h:commandLink id="clinkexcel" target="_blank" value="" 
							action="#{invoiceReportByPlateBeanAdmin.printPdf}">
							<img title="Imprimir" src="/web.bo.peajes.com/img/generar_xls_btn.gif" alt="Imprimir"
							style="cursor: pointer; border: 0px solid blue;" />
                            <f:setPropertyActionListener target="#{invoiceReportByPlateBeanAdmin.exportOption}" value="EXCEL"/>
		           		</h:commandLink>
					</h:panelGrid>					
				</rich:panel>
			</a4j:outputPanel>
      </rich:panel>
    </h:form>

    <rich:modalPanel id="panel" width="370" height="118" showWhenRendered="#{invoiceReportByPlateBeanAdmin.showMessageError}" onresize="return false;" moveable="false">
		<f:facet name="header">
			<h:panelGroup><h:outputText value="Mensaje" styleClass="bold"/></h:panelGroup>
		</f:facet>
		<f:facet name="controls">
			<h:panelGroup>
				<a4j:form>
					<h:commandButton action="#{invoiceReportByPlateBeanAdmin.init}" image="/img/close.png" />
				</a4j:form>
			</h:panelGroup>
		</f:facet>
		<h:outputText value="#{invoiceReportByPlateBeanAdmin.messageError}" styleClass="normal"/>
		<h:form>
			<h:panelGrid columns="1" styleClass="tableBO" style="width: 100px; margin: auto; padding-left:8px; ">
		     	<h:panelGroup>
		     		<h:commandButton value="Aceptar" action="#{invoiceReportByPlateBeanAdmin.init}"  />
		     	</h:panelGroup>	     	
		    </h:panelGrid>
		</h:form>		
		<br />
	</rich:modalPanel>

</div>
<f:subview id="footer">
	<ui:include src="/jsf/content/footer.jsp"/>
</f:subview>
</f:view>
</div>
</body>
</html>