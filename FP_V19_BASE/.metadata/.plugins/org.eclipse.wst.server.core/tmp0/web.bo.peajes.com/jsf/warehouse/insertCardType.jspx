<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:a4j="http://richfaces.org/a4j">
<head>
</head>

<body>
<div class="container">
<f:view>
<div>
	<rich:messages style="color:red;" styleClass="bold"/>		
	<h:form id="Formulario">
		<f:loadBundle basename="staticText.messages" var="msg" />
		<rich:panel style="width:350px;">
			<table width="100%">
				<tr>
					<td width="10%"></td>
					<td width="50%">
						<h:outputText value="Digite el Tipo de Tarjeta " styleClass="bold"/>
						<h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
					</td>
					<td width="40%">
						<h:inputText value="#{warehouseCardTypeBean.cardType}" required="true" maxlength="50"
							id="Nombre" validatorMessage="El campo debe ser máximo de 50 caracteres."
							requiredMessage="El Nombre del tipo de tarjeta es necesario.">
							<f:validateLength maximum="50"/>
							<f:validator validatorId="blank"/>
						</h:inputText>
					</td>
				</tr>
				<tr>
					<td colspan="2">
						<h:outputLabel value="#{msg.Asterisk}" styleClass="asteriskMandatoryField"/>
						<h:outputLabel value="#{msg.MandFields}" styleClass="loginInput"/>
					</td>
					<td width="60%"><h:commandButton value="Crear"  styleClass="bold" 
						action="#{warehouseCardTypeBean.addCardType}"/></td>
				</tr>
			</table>

		</rich:panel>
	</h:form>
	<rich:spacer height="20"/>	

</div>
</f:view>
</div>
</body>
</html>