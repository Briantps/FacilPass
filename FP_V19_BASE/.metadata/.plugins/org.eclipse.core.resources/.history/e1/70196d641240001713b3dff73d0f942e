<script type="text/javascript">
	function getFocus(numero) {
		debugger;
		if (numero == 1){
			var elem = document.getElementById("formPrincipal:balanceAcount");
			elem.value = "";
   			setTimeout(function(){elem.focus();},1000);
		} else if (numero == 2){
			setTimeout(function(){document.getElementById("formPrincipal:assingRecharge").focus();},100);
		} else if (numero == 3){
			setTimeout(function(){document.getElementById("formPrincipal:pCalendar").focus();},100);
		}

		return true;
	}
</script>
