package ejb.taskeng.email.impl;

import ejb.taskeng.email.Message;

public class MessageBase implements Message {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private int id;
	private String content;
	
	public MessageBase(){
	}
	
	/* (non-Javadoc)
	 * @see email.Message#getId()
	 */
	public int getId() {
		return id;
	}
	/* (non-Javadoc)
	 * @see email.Message#setId(int)
	 */
	public void setId(int id) {
		this.id = id;
	}
	/* (non-Javadoc)
	 * @see email.Message#getContent()
	 */
	public String getContent() {
		return content;
	}
	/* (non-Javadoc)
	 * @see email.Message#setContent(java.lang.String)
	 */
	public void setContent(String content) {
		this.content = content;
	}	
}
