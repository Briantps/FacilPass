package util;

import java.io.Serializable;

import jpa.ReOptionAction;

/**
 * @author Frances Zucchet
 * 
 */
public class Permission implements Serializable {
	
	private static final long serialVersionUID = -3896510521529736462L;
	
	private ReOptionAction optAct;
	
	private boolean active;

	/**
	 * Default constructor
	 */
	public Permission(ReOptionAction optAct, boolean active) {
		this.optAct = optAct;
		this.active = active;
	}

	/**
	 * @return the optAct
	 */
	public ReOptionAction getOptAct() {
		return optAct;
	}

	/**
	 * @param optAct the optAct to set
	 */
	public void setOptAct(ReOptionAction optAct) {
		this.optAct = optAct;
	}

	/**
	 * @return the active
	 */
	public boolean isActive() {
		return active;
	}

	/**
	 * @param active the active to set
	 */
	public void setActive(boolean active) {
		this.active = active;
	}
}
