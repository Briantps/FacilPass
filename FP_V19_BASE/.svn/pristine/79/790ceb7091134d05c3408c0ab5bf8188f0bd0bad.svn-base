package util;

import java.io.Serializable;

import jpa.TbRole;

/**
 * @author Frances Zucchet
 * 
 */
public class UsrRole implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private TbRole role;
	private boolean active;

	/**
	 * Constructor.
	 */
	public UsrRole(TbRole role, boolean active) {
		this.role = role;
		this.active = active;
	}

	/**
	 * @return the role
	 */
	public TbRole getRole() {
		return role;
	}

	/**
	 * @param role
	 *            the role to set
	 */
	public void setRole(TbRole role) {
		this.role = role;
	}

	/**
	 * @return the active
	 */
	public boolean isActive() {
		return active;
	}

	/**
	 * @param active
	 *            the active to set
	 */
	public void setActive(boolean active) {
		this.active = active;
	}
}
